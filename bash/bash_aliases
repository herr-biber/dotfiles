alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias ssh='ssh -c arcfour,blowfish-cbc -XC'
alias ..='cd ..'
alias ...='cd ../..'
# cd into most recent subdir
alias cdnew='cd $(ls -d  -tr -- */| tail -1)'
alias gedit='gedit 2> /dev/null'
alias evince='evince 2> /dev/null'
alias lsabs='find $PWD -maxdepth 1 -type f'
alias llabs='lsabs | xargs ls -l'

# cd to target of link
function cdlink
{
    cd $(readlink $1)
}
